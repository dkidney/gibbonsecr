citHeader("To cite gibbonsecr in publications use:")

year = sub(".*(2[[:digit:]]{3})-.*", "\\1", meta$Date)
version = paste("R package version", meta$Version)

citEntry(
  entry  = "Manual",
  title  = "gibbonsecr: SECR Analysis For Acoustic Gibbon Surveys",
  author = "Darren Kidney",
  year   = year,
  note   = version,

  textVersion =
  paste0("Kidney, D. (", year, "). ",
        "gibbonsecr: SECR Analysis For Acoustic Gibbon Surveys. ",
        version, ".")
)